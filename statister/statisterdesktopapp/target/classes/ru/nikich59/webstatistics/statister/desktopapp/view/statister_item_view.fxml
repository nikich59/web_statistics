<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/8.0.112"
		 xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox
				style=
						"-fx-border-width: 1;
						-fx-padding: 8;
						-fx-border-color: black;">
			<children>
				<Label fx:id="descriptionLabel" maxWidth="400">

				</Label>
				<Label
						fx:id="urlLabel" maxWidth="400">

				</Label>
				<Label fx:id="errorLabel" maxWidth="400">

				</Label>
				<HBox>
					<children>
						<Label fx:id="periodLabel">

						</Label>
						<Label fx:id="">

						</Label>
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</fx:root>

